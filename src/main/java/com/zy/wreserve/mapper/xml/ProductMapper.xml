<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zy.wreserve.mapper.ProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zy.wreserve.wechat.entity.Product">
        <id column="product_id" property="productId" />
        <result column="product_title" property="productTitle" />
        <result column="product_name" property="productName" />
        <result column="product_menu_id" property="productMenuId" />
        <result column="product_price" property="productPrice" />
        <result column="product_img" property="productImg" />
        <result column="sales" property="sales" />
        <result column="inventory" property="inventory" />
        <result column="is_recommend" property="isRecommend" />
        <result column="product_detail_img" property="productDetailImg" />
    </resultMap>

</mapper>
